<div class="heroContainer">
  <div class="card transparent z-depth-5 hero_overlay">
    <p>Discover the Architecture around you.</p>
    <a href="/explore" class="waves-effect waves-teal btn-flat" style="background-color: #ee6e73; color: #FFF">Explore</a>
  </div>

</div>



  <div class="discoveryContainer">
    <h5>Recently Discovered</h5>
      {{#each spots}}

      <div class="wrapper">
        <img class="responsive-img img" src="{{this.imgURL}}">
        <div class="img_overlay">
          <p>{{this.coordinates.lat}},  {{this.coordinates.lng}}</p>
        </div>
      </div>

      {{/each}}
  </div>
</div>
<script>
  $(document).ready( function() {
    $('.button-collapse').sideNav();
  })
</script>
