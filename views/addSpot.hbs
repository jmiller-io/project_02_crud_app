<h1>{{title}}</h1>
<section class="insert #efebe9 brown lighten-5">
    <form action="/addSpot" method="post" enctype="multipart/form-data">
      <div class="input">
        <label for="img">Image</label>
        <input type="file" name="myimg" required>
      </div>
      <div class="input">
        <label for="location">Location</label>
        <input type="text" placeholder="Enter Latitude" id="lat" name="lat" required>
        <input type="text" placeholder="Enter Longitude" id="lng" name="lng" required>
      </div>
      <div class="input">
        <label for="description">Description</label>
        <input type="text" id="description" name="description" placeholder="Enter a Note">
      </div>
      <div class="input-field col s12">
        <select id="categories" name="category">
          <option value="" disabled selected>Choose a Category</option>
          <option value="A-Frame">A-Frame</option>
          <option value="Art Deco">Art Deco</option>
          <option value="Craftsman">Craftsman Bungalow</option>
          <option value="Colonial">Colonial</option>
          <option value="Mid-Century Modern">Mid-Century Modern</option>
          <option value="Mediterranean">Mediterranean</option>
          <option value="Modern">Modern</option>
          <option value="Prairie">Prairie</option>
          <option value="Ranch">Ranch</option>
          <option value="Tudor">Tudor</option>
          <option value="Spanish">Spanish</option>
          <option value="Victorian">Victorian</option>
          <option value="Oriental">Oriental</option>
        </select>
      </div>
      <button class="btn-floating waves-effect waves-light red" type="submit" name="action">
        <i class="material-icons right">add</i>
      </button>
    </form>
</section>
<script src="/addSpot.js"></script>
